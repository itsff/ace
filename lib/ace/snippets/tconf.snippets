snippet in
	in [${1:value1}, ${2:value2} ]
snippet find
	find(${1:value1}, ${2:value2} )
snippet copy
	copy(${1:pub.fid1}, [${2:pub.fid2}, ${3:pub.fid3}] )
snippet exists
	exists(${1:src.FID}) == ${2:#true}
snippet strtime
	strtime(${1:"2116-12-20 22:00:00.000"})
snippet map
	map(${1:src.trd_prc_1}, {${2:key1}\:${3:value1},${4:key2}\:${5:value2}})
snippet convert_date
	convert_date(time_service, ${1:#GMTDATE}, ${2:#GMTTIME}, ${3:"MUT\%DST"})
snippet get_prev_exch_trading_day
	get_prev_exch_trading_day(time_service, ${1:#GMTDATE}, ${2:#GMTTIME},${3:"MUT\%DST"},${4:"MUT\%FD01"}, ${5:"MUT\%HOL1"})
snippet rescale
	rescale(${1: veh.nav_netchn/ veh.navalue * 100}, ${2:-6} )
